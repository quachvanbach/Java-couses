
public class TimKiemChuoi {
	public static void main(String[] args) {
		String s1 = "Họ tên: Nguyễn Văn A. Ngày sinh: 01-01-1991. Địa chỉ: Thanh Xá, Thanh Hà, Hải Dương";
		String s2 = "Họ tên: Mạc Văn B. Ngày sinh: 02-02-1992. Địa chỉ: Thanh Thuỷ, Thanh Hà, Hải Dương";

		// Hàm indexOf => trả lại vị trí của nội dung được tìm kiếm trong chuỗi đó (nếu
		// có).
		System.out.println(s1.indexOf("Thanh Xá, Thanh Hà, Hải Dương"));
		System.out.println(s2.indexOf("Thanh Xá, Thanh Hà, Hải Dương"));

		// Hàm indexOf có sử dụng vị trí bắt đầu tìm kiếm:
		System.out.println(s1.indexOf("A", 19));
		System.out.println(s2.indexOf("B", 19));
	}
}
