package view;

import javax.swing.JFrame;

public class GioiThieuVeJFRAME {
	public static void main(String[] args) {
		JFrame jf = new JFrame();
		jf.setVisible(true);
		jf.setSize(600,400);
		jf.setTitle("Ví dụ về JFrame");
		
		while(true) {
			int i = 1;
			i+=1;
			System.out.println(i);
		}
	}
}
