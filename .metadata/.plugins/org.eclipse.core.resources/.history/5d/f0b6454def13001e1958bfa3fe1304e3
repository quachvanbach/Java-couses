
public class ContinueKeyword {
	public static void main(String[] args) {
		int i, j;
		System.out.println("*****************************************");
		System.out.println("---------- Không sử dụng label ----------");
		for (i = 0; i < 10; i++) {

			for (j = 0; j <= 3; j++) {
				if (j < 5)
					continue;
				System.out.println("i = " + i + " và j =" + j);
			}
		}
		System.out.println("*****************************************");

		System.out.println("------------ Sử dụng label -------------");

		System.out.println("*****************************************");
	}
}
