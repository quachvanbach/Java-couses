import java.util.Scanner;

public class KiemTraSoNgayTrongThang {
	public static void main(String[] args) {
		int thang, nam;
		Scanner sc = new Scanner(System.in);
		System.out.println("Nhập vào tháng: ");
		
		System.out.println("Nhập vào tháng: ");
		
		switch (thang) {
		case 1:
		case 3:
		case 5:
		case 7:
		case 8:
		case 10:
		case 12:
			System.out.println("Có 31 ngày");
			break;

		case 4:
		case 6:
		case 9:
		case 11:
			break;
		default:
			break;
		}
	}
}
