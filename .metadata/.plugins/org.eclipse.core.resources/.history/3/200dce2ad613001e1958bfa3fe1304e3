import java.util.Scanner;

public class VongLapFor {
	public static void main(String[] args) {
		int x, y;
		String tenHocSinh;
		Scanner sc = new Scanner(System.in);
		System.out.println("Nhập vào x khởi tạo:");
		int x1 = sc.nextInt();
		System.out.println("Nhập vào y:");
		y = sc.nextInt();
		System.out.println("Nhập vào tên học sinh: ");
		tenHocSinh = sc.nextLine();
		for (x = x1 ; x <= y; x++) {
			System.out.println("Nhập vào tên của học sinh");
			System.out.println("Đã nhập học sinh thứ " + x + " với tên là "+ tenHocSinh);
		}
	}
}
