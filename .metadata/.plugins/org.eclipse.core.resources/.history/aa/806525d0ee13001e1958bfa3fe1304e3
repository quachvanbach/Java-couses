
public class ContinueKeyword {
	public static void main(String[] args) {
		int i;
		System.out.println("*****************************************");
		System.out.println("---------- Không sử dụng label ----------");
		for (i = 0; i <= 10; i++) {
			if (i < 5)
				continue;
			System.out.println(i);
		}
		System.out.println("Bên ngoài vòng lặp out: out = " + out + ", in = " + in);
		System.out.println("*****************************************");

	}
}
