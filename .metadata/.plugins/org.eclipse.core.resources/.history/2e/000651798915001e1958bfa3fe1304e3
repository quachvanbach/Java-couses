
public class Dog extends Animal{
	public  public Dog() {
		super(name);
		// TODO Auto-generated constructor stub
	}


	@Override
	public void eat() {
		System.out.println("Dog eats bones...");
		super.eat();
	}

	@Override
	public void makeSound() {
		System.out.println("Gau gau");
		super.makeSound();
	}
}
