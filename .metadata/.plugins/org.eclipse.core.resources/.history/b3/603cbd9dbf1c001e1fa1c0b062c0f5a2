		this.setTitle("Thành phần và bố cục giao diện");
		this.setSize(600,400);
		this.setLocationRelativeTo(null); //=> Căn cửa sổ ở giữa màn hình.
		
		FlowLayout flowLayout = new FlowLayout();
		
		JButton jButton_1 = new JButton();
		JButton jButton_2 = new JButton();
		JButton jButton_3 = new JButton();
		
		this.setVisible(true);
	}

	public static void main(String[] args) {
		ThanhPhanBoCucGiaoDien giaoDien = new ThanhPhanBoCucGiaoDien();
	}
}
