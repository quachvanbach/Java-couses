
public class ContinueKeyword {
	public static void main(String[] args) {
		int i, j;
		System.out.println("*****************************************");
		System.out.println("---------- Không sử dụng label ----------");
		for (j = 0; j <= 10; j++) {
			if (j < 5)
				continue;
			System.out.println(j);
		}
		System.out.println("*****************************************");

		System.out.println("------------ Sử dụng label -------------");
		
		System.out.println("*****************************************");
	}
}
