
public class Ngay {
	private int ngay, thang, nam;

	public Ngay(int ngay, int thang, int nam) {
		this.ngay = ngay;
		this.thang = thang;
		this.nam = nam;
	}

	public int getNgay(int ngay) {
		return ngay;
	}

	public void setNgay(int ngay) {
		this.ngay = ngay;
	}

	public int getThang(int thang) {
		return thang;
	}
	public void setThang(int thang) {
		this.thang = thang;
	}
}
