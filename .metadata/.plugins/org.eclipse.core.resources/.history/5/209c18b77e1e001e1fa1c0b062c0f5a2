package model;

public class LastButtonModel {
	private int value;

	public LastButtonModel(int value) {
		this.value = value;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}

	public void setValue_1() {
		this.setValue(1);
	}

	public void setValue_2() {
		this.setValue(2);
	}
}
